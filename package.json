{
  "name": "split",
  "private": true,
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
